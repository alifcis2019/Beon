#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/nuxt@4.2.2_@parcel+watcher@2.5.1_@types+node@24.9.1_@vue+compiler-sfc@3.5.25_better-sql_d9ddbd7439cac68dd5805ec37d773599/node_modules/nuxt/bin/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/nuxt@4.2.2_@parcel+watcher@2.5.1_@types+node@24.9.1_@vue+compiler-sfc@3.5.25_better-sql_d9ddbd7439cac68dd5805ec37d773599/node_modules/nuxt/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/nuxt@4.2.2_@parcel+watcher@2.5.1_@types+node@24.9.1_@vue+compiler-sfc@3.5.25_better-sql_d9ddbd7439cac68dd5805ec37d773599/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/nuxt@4.2.2_@parcel+watcher@2.5.1_@types+node@24.9.1_@vue+compiler-sfc@3.5.25_better-sql_d9ddbd7439cac68dd5805ec37d773599/node_modules/nuxt/bin/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/nuxt@4.2.2_@parcel+watcher@2.5.1_@types+node@24.9.1_@vue+compiler-sfc@3.5.25_better-sql_d9ddbd7439cac68dd5805ec37d773599/node_modules/nuxt/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/nuxt@4.2.2_@parcel+watcher@2.5.1_@types+node@24.9.1_@vue+compiler-sfc@3.5.25_better-sql_d9ddbd7439cac68dd5805ec37d773599/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
else
  exec node  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
fi
