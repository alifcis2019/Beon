#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/prebuild-install/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules:/home/ali/work/beon-landing-nuxt/Beon/node_modules:/home/ali/work/beon-landing-nuxt/node_modules:/home/ali/work/node_modules:/home/ali/node_modules:/home/node_modules:/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/prebuild-install/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules:/home/ali/work/beon-landing-nuxt/Beon/node_modules:/home/ali/work/beon-landing-nuxt/node_modules:/home/ali/work/node_modules:/home/ali/node_modules:/home/node_modules:/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../prebuild-install/bin.js" "$@"
else
  exec node  "$basedir/../prebuild-install/bin.js" "$@"
fi
