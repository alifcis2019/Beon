#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/@nuxt+ui@4.2.1_@babel+parser@7.28.5_@nuxt+content@3.8.2_better-sqlite3@12.5.0_magicast@_fc4915e8f02cf786a5297e6833cca1a2/node_modules/@nuxt/ui/cli/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/@nuxt+ui@4.2.1_@babel+parser@7.28.5_@nuxt+content@3.8.2_better-sqlite3@12.5.0_magicast@_fc4915e8f02cf786a5297e6833cca1a2/node_modules/@nuxt/ui/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/@nuxt+ui@4.2.1_@babel+parser@7.28.5_@nuxt+content@3.8.2_better-sqlite3@12.5.0_magicast@_fc4915e8f02cf786a5297e6833cca1a2/node_modules/@nuxt/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/@nuxt+ui@4.2.1_@babel+parser@7.28.5_@nuxt+content@3.8.2_better-sqlite3@12.5.0_magicast@_fc4915e8f02cf786a5297e6833cca1a2/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/@nuxt+ui@4.2.1_@babel+parser@7.28.5_@nuxt+content@3.8.2_better-sqlite3@12.5.0_magicast@_fc4915e8f02cf786a5297e6833cca1a2/node_modules/@nuxt/ui/cli/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/@nuxt+ui@4.2.1_@babel+parser@7.28.5_@nuxt+content@3.8.2_better-sqlite3@12.5.0_magicast@_fc4915e8f02cf786a5297e6833cca1a2/node_modules/@nuxt/ui/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/@nuxt+ui@4.2.1_@babel+parser@7.28.5_@nuxt+content@3.8.2_better-sqlite3@12.5.0_magicast@_fc4915e8f02cf786a5297e6833cca1a2/node_modules/@nuxt/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/@nuxt+ui@4.2.1_@babel+parser@7.28.5_@nuxt+content@3.8.2_better-sqlite3@12.5.0_magicast@_fc4915e8f02cf786a5297e6833cca1a2/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@nuxt/ui/cli/index.mjs" "$@"
else
  exec node  "$basedir/../@nuxt/ui/cli/index.mjs" "$@"
fi
