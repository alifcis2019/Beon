#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/@nuxt+cli@3.31.2_cac@6.7.14_magicast@0.5.1/node_modules/@nuxt/cli/bin/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/@nuxt+cli@3.31.2_cac@6.7.14_magicast@0.5.1/node_modules/@nuxt/cli/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/@nuxt+cli@3.31.2_cac@6.7.14_magicast@0.5.1/node_modules/@nuxt/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/@nuxt+cli@3.31.2_cac@6.7.14_magicast@0.5.1/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/@nuxt+cli@3.31.2_cac@6.7.14_magicast@0.5.1/node_modules/@nuxt/cli/bin/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/@nuxt+cli@3.31.2_cac@6.7.14_magicast@0.5.1/node_modules/@nuxt/cli/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/@nuxt+cli@3.31.2_cac@6.7.14_magicast@0.5.1/node_modules/@nuxt/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/@nuxt+cli@3.31.2_cac@6.7.14_magicast@0.5.1/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@nuxt+cli@3.31.2_cac@6.7.14_magicast@0.5.1/node_modules/@nuxt/cli/bin/nuxi.mjs" "$@"
else
  exec node  "$basedir/../../../../../@nuxt+cli@3.31.2_cac@6.7.14_magicast@0.5.1/node_modules/@nuxt/cli/bin/nuxi.mjs" "$@"
fi
