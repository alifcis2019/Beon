#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/vite@7.2.7_@types+node@24.9.1_jiti@2.6.1_lightningcss@1.30.2_terser@5.44.0_yaml@2.8.2/node_modules/vite/bin/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/vite@7.2.7_@types+node@24.9.1_jiti@2.6.1_lightningcss@1.30.2_terser@5.44.0_yaml@2.8.2/node_modules/vite/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/vite@7.2.7_@types+node@24.9.1_jiti@2.6.1_lightningcss@1.30.2_terser@5.44.0_yaml@2.8.2/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/vite@7.2.7_@types+node@24.9.1_jiti@2.6.1_lightningcss@1.30.2_terser@5.44.0_yaml@2.8.2/node_modules/vite/bin/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/vite@7.2.7_@types+node@24.9.1_jiti@2.6.1_lightningcss@1.30.2_terser@5.44.0_yaml@2.8.2/node_modules/vite/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/vite@7.2.7_@types+node@24.9.1_jiti@2.6.1_lightningcss@1.30.2_terser@5.44.0_yaml@2.8.2/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
