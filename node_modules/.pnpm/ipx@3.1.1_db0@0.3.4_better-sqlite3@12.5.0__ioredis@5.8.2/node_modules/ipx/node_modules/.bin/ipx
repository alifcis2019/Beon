#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/ipx@3.1.1_db0@0.3.4_better-sqlite3@12.5.0__ioredis@5.8.2/node_modules/ipx/bin/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/ipx@3.1.1_db0@0.3.4_better-sqlite3@12.5.0__ioredis@5.8.2/node_modules/ipx/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/ipx@3.1.1_db0@0.3.4_better-sqlite3@12.5.0__ioredis@5.8.2/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/ipx@3.1.1_db0@0.3.4_better-sqlite3@12.5.0__ioredis@5.8.2/node_modules/ipx/bin/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/ipx@3.1.1_db0@0.3.4_better-sqlite3@12.5.0__ioredis@5.8.2/node_modules/ipx/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/ipx@3.1.1_db0@0.3.4_better-sqlite3@12.5.0__ioredis@5.8.2/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/ipx.mjs" "$@"
else
  exec node  "$basedir/../../bin/ipx.mjs" "$@"
fi
