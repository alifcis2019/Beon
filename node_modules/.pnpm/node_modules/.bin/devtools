#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/@nuxt+devtools@3.1.1_vite@7.2.7_@types+node@24.9.1_jiti@2.6.1_lightningcss@1.30.2_terse_a8a67e7aa8f51f7120f8969ab672c70d/node_modules/@nuxt/devtools/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/@nuxt+devtools@3.1.1_vite@7.2.7_@types+node@24.9.1_jiti@2.6.1_lightningcss@1.30.2_terse_a8a67e7aa8f51f7120f8969ab672c70d/node_modules/@nuxt/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/@nuxt+devtools@3.1.1_vite@7.2.7_@types+node@24.9.1_jiti@2.6.1_lightningcss@1.30.2_terse_a8a67e7aa8f51f7120f8969ab672c70d/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/@nuxt+devtools@3.1.1_vite@7.2.7_@types+node@24.9.1_jiti@2.6.1_lightningcss@1.30.2_terse_a8a67e7aa8f51f7120f8969ab672c70d/node_modules/@nuxt/devtools/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/@nuxt+devtools@3.1.1_vite@7.2.7_@types+node@24.9.1_jiti@2.6.1_lightningcss@1.30.2_terse_a8a67e7aa8f51f7120f8969ab672c70d/node_modules/@nuxt/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/@nuxt+devtools@3.1.1_vite@7.2.7_@types+node@24.9.1_jiti@2.6.1_lightningcss@1.30.2_terse_a8a67e7aa8f51f7120f8969ab672c70d/node_modules:/home/ali/work/beon-landing-nuxt/Beon/Saas/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@nuxt/devtools/cli.mjs" "$@"
else
  exec node  "$basedir/../@nuxt/devtools/cli.mjs" "$@"
fi
